<div id="fullpage">
	<div class="section">
		<div class="welcome">
			
			<div class="container">
				<div class="row" id="icon">
					<div class="col-md-7 col-md-offset-2" id="small-push">
						<%=image_tag "pup.png", :class => "dog" %>
						<%=image_tag "title-dog.png", :class => "title" %>
					</div>

					<div class="col-md-3 col-md-pull-2" id="large-push">
						<div class="btn-group btn-group-vertical" role="group" aria-label="...">
						  <div class="btn-group" role="group">
						   <a href="#availability"> <button type="button" class="btn btn-info">Book A Meet and Greet</button></a>
						  </div>
						  <div class="btn-group" role="group">
						    <button type="button" class="btn btn-info">Schedule</button>
						  </div>
						  <a href="#faq">
						  <div class="btn-group" role="group">
						    <button type="button" class="btn btn-info">Learn More</button>
						  </div>
						</a>
						</div>
					</div>
					<div class="row">
						<div class="col-md-8 col-md-offset-3" id="slog">
							<h2>SERVING SOMERVILLE AND EAST CAMBRIDGE</h2>
						</div>
					</div>
				 </div>	  
			</div>
		</div>
			
	</div>
   
	
 	<a name="availability"></a>
    <div class="section">
    	<div class="calendar">
    		<h1>AVAILABILITY</h1>
				<div id="calendar" class="fc fc-ltr fc-unthemed"></div>
		</div>
	</div>

	<a name="faq"></a>
	<div class="section">
		<h1>FAQ</h1>
		<div class="container">
			<div class="row">
				<%= render 'faq' %>
			</div>
		</div>
		<a name="servicesandrates"></a>
		<div class="section rates">
			<h1>Services and Rates</h1>
				<div class="container">
					<div class="row">
						<%= render 'servicesrates' %>
					</div>
				</div>
			</div>
			<a name="photos"></a>
			<div class="section photos">
			<h1>Photos</h1>
				<div class="container">
					<div class="row">
						<div class="col-md-4 col-md-offset-2">

							<%= image_tag "pierre.jpg", :class => 'pierre' %>
						</div>
						<div class="col-md-4">
							<%= image_tag "snowface.jpg", :class=> 'snowface' %>
						</div>
							
						</div>
					</div>
				</div>
			</div>

			<a name="contact"></a>
			<div class="section rates">
			<h1>Contact</h1>
				<div class="container">
					<div class="row">
						<div class="col-md-8 col-md-offset-2">
							<h2>Questions? Email:</h2>
							<h3><%= mail_to "ali@swagdogwalking.com" %></h3>
							<br />
							<h3>or</h3> 
							<br />
							<h2>Fill Out a Form For Additional Info:</h2>
							
						</div>
					</div>
				</div>
			</div>
	


	</div>

	

</div>
