<!DOCTYPE html>
<html>
<head>
  <title>Swag Dog Walking</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag 'application', 'https://fonts.googleapis.com/css?family=Raleway', :media => "all" %>
  <%= stylesheet_link_tag 'application', 'https://fonts.googleapis.com/css?family=Dosis:300,400', :media => "all" %>
  <%= csrf_meta_tags %>
  <meta name="google-site-verification" content="MY4PV8THpEkVqJs2rbrGDpuZ-z2Wv7cpnB4PvVELfPc" />
</head>

	<nav class="navbar navbar-default">
	  <div class="container-fluid">
	    <!-- Brand and toggle get grouped for better mobile display -->
	    <div class="navbar-header">
	      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
	        <span class="sr-only">Toggle navigation</span>
	        <span class="icon-bar"></span>
	        <span class="icon-bar"></span>
	        <span class="icon-bar"></span>
	      </button>
	      <a class="navbar-brand" href="/">SWAG DOG WALKING</a>
	    </div>

	    <!-- Collect the nav links, forms, and other content for toggling -->
	    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
	      <ul class="nav navbar-nav">
	        <li class="active"><a href="#">ABOUT <span class="sr-only">(current)</span></a></li>
	        <li><a href="#faq">FAQ</a></li>
	        <li class="dropdown">
	          <a href="#servicesandrates" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">SERVICES AND RATES <span class="caret"></span></a>
	          <ul class="dropdown-menu" role="menu">
	            <li><%= link_to "Services", "/#servicesandrates" %></li>
	            <li><%= link_to "Rates", "/#servicesandrates" %></li>
	            <li><%= link_to "Get Started", getstarted_path %> </li>
	            <li class="divider"></li>
	            <li><li><%= link_to "Contact", "/#contact" %></li></li>
	          </ul>
	        </li>
	      </ul>
	      <ul class="nav navbar-nav navbar-right">
	      	<% if model_signed_in? %>
            <li><%= link_to "SIGN OUT", destroy_model_session_path, :method => :delete %></li>
             <% else %>
             <li><%= link_to "SIGN UP", new_model_registration_path  %></li>
             <% end %>
	        <li><a href="#photos">PHOTOS</a></li>
	        <li><%= link_to 'BLOG', "/blog" %></li>
	      	<li> 
	      	<div class="fb-like"
  			data-share="true"
  			data-width="450"
  			data-show-faces="true">
			</div>
			</li>
	       
	          </ul>
	        </li>
	      </ul>
	    </div><!-- /.navbar-collapse -->
	  </div><!-- /.container-fluid -->
	</nav>

<body>
	




<%= yield %>


<script>
  window.fbAsyncInit = function() {
    FB.init({
      appId      : '966206066732116',
      xfbml      : true,
      version    : 'v2.2'
    });
  };

  (function(d, s, id){
     var js, fjs = d.getElementsByTagName(s)[0];
     if (d.getElementById(id)) {return;}
     js = d.createElement(s); js.id = id;
     js.src = "//connect.facebook.net/en_US/sdk.js";
     fjs.parentNode.insertBefore(js, fjs);
   }(document, 'script', 'facebook-jssdk'));
</script>

</body>



</html>
